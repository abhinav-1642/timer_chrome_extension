  {
    "manifest_version": 2,

    "name": "Simple Timer ",
    "description": "Set intervals of work and rest to maximize productivity",
    "version": "0.0.0.5",
    "author": "Abhinav",
    "options_page": "options.html",

    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["options.js"],
      "css": ["style.css"]
    }],

    "browser_action": {
      "default_popup": "xd.html"
    }
  }
